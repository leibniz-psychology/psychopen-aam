msgid "plugins.generic.aam.help.content"
msgstr "Articles below are \"in press\", i.e., accepted for publication in <em>{$journalName}</em> but not yet published. If authors did make their accepted version available via <a href=\"https://www.psycharchives.org/\" target=\"_blank\">PsychArchives</a> a link to the author accepted manuscript (AAM) version is provided."
       
msgid "plugins.generic.aam.help.submit"
msgstr "<strong>@Authors:</strong> Do you want to provide early access to your research? Please <a href=\"https://www.psychopen.eu/AAM_Form.docx\">download and fill in this form</a> and send it along with your accepted manuscript to <a href=\"mailto:psycharchives-submission@leibniz-psychology.org\">psycharchives-submission@leibniz-psychology.org</a>."
       
